    function loading(){
        var a=parseInt(Math.random()*1000);   
        $(".biaoge_gexuan").load("/Handler/wsyy/GetShiDuan.ashx?codeid="+codeid+"&a="+a+"");
        var aaa = $(".biaoge_gexuan").html();     
           
